    "@kintone/webpack-plugin-kintone-plugin": "^3.0.1",
    "babel-loader": "^8.0.6",
    "env-cmd": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "core-js": "3"
  }
